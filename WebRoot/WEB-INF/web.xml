<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>WeChat</servlet-name>
    <servlet-class>servlet.WeChat</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ViewTest</servlet-name>
    <servlet-class>servlet.ViewTest</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>test</servlet-name>
    <servlet-class>servlet.BindServlet</servlet-class>
  </servlet>

  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>regist</servlet-name>
    <servlet-class>servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>login_web</servlet-name>
    <servlet-class>servlet_web.login_web</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>changedata_web</servlet-name>
    <servlet-class>servlet_web.changedata_web</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>RegistWeb</servlet-name>
    <servlet-class>servlet_web.RegistWeb</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>regist_web</servlet-name>
    <servlet-class>servlet_web.regist_web</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>WeChat</servlet-name>
    <url-pattern>/yanzheng</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ViewTest</servlet-name>
    <url-pattern>/ViewTest</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>	
  <servlet-mapping>
    <servlet-name>regist</servlet-name>
    <url-pattern>/regist</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>login_web</servlet-name>
    <url-pattern>/login_web</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>changedata_web</servlet-name>
    <url-pattern>/changedata_web</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegistWeb</servlet-name>
    <url-pattern>/RegistWeb</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>regist_web</servlet-name>
    <url-pattern>/regist_web</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
